{
	"name": "@ultraq/react-async-transition-component",
	"version": "1.1.0",
	"description": "A container for react-async-component for performing component transitions",
	"author": "Emanuel Rabina <emanuelrabina@gmail.com> (http://www.ultraq.net.nz/)",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ultraq/react-async-transition-component.git"
	},
	"module": "AsyncTransitionComponent.es.js",
	"main": "AsyncTransitionComponent.cjs.js",
	"sideEffects": false,
	"scripts": {
		"lint": "eslint \"*.js\"",
		"test": "jest",
		"build": "npm run build:cjs && npm run build:es",
		"build:cjs": "cross-env BABEL_ENV=cjs babel AsyncTransitionComponent.js --out-file AsyncTransitionComponent.cjs.js --source-maps",
		"build:es": "cross-env BABEL_ENV=es babel AsyncTransitionComponent.js --out-file AsyncTransitionComponent.es.js --source-maps",
		"prepublishOnly": "npm run build"
	},
	"peerDependencies": {
		"prop-types": "15",
		"react": "16"
	},
	"dependencies": {
		"@ultraq/react-async-component": "^1.1.0",
		"classnames": "^2.3.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.15.7",
		"@babel/core": "^7.15.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.15.6",
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-react": "^7.14.5",
		"babel-eslint": "^10.1.0",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"cross-env": "^7.0.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"eslint": "^7.32.0",
		"eslint-config-ultraq": "^2.4.0",
		"eslint-plugin-react": "^7.26.1",
		"jest": "^27.2.4",
		"prop-types": "^15.7.2",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-test-renderer": "^16.14.0"
	},
	"engines": {
		"node": ">=10"
	}
}
